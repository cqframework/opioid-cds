<Encounter xmlns="http://hl7.org/fhir">
    <extension url="http://fhir.org/guides/cdc/opioid-cds/StructureDefinition/dataDateRoller">
        <extension url="dateLastUpdated">
            <valueDateTime value="2022-04-28"/>
        </extension>
        <extension url="frequency">
            <valueDuration>
                <value value="30.0"/>
                <unit value="days"/>
                <system value="http://unitsofmeasure.org"/>
                <code value="d"/>
            </valueDuration>
        </extension>
    </extension>
    <id value="example-rec-07-six-of-past-ten-days-prefetch"/>
    <status value="finished"/>
    <subject>
        <reference value="Patient/example-rec-07-six-of-past-ten-days"/>
    </subject>
    <period>
        <start>2022-04-18</start>
        <end>2022-04-18</end>
        <extension url="http://fhir.org/guides/cdc/opioid-cds/StructureDefinition/cdc-valueset-query">
            <extension url="language">
                <valueUri value="text/cql"/>
            </extension>
            <extension url="expression">
                <valueString
                        value="FHIR.Period { start: FHIR.dateTime { value: Today() - 10 days }, end: FHIR.dateTime { value: Today() - 10 days } }"/>
            </extension>
        </extension>
    </period>
</Encounter>
